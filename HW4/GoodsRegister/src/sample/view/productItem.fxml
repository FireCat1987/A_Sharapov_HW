<columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="190.0" prefWidth="190.0" />
</columnConstraints>
<rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
</rowConstraints>
<children>
    <Label fx:id="productId" text="Label" GridPane.columnIndex="0" />
    <Label fx:id="productName" text="Label" GridPane.columnIndex="1" />
</children>
</GridPane>
